{
  "name": "@brimble/models",
  "version": "1.2.7-alpha.0",
  "description": "Brimble models",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p .",
    "pre-commit": "precise-commits && lint-staged",
    "pre-push": "lint-prepush"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "git add -A ."
    ],
    "*.{json,md,js,ts}": [
      "prettier --write"
    ]
  },
  "lint-prepush": {
    "base": "develop",
    "tasks": {
      "*.{ts,js}": [
        "eslint"
      ]
    }
  },
  "license": "MIT",
  "dependencies": {
    "@brimble/utils": "^1.2.7-alpha.0",
    "dotenv": "^8.2.0",
    "mongoose": "^5.8.11",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/node": "^14.0.13",
    "@types/uuid": "^8.3.0",
    "lint-prepush": "^2.2.0",
    "lint-staged": "^12.3.4",
    "nodemon": "^2.0.2",
    "precise-commits": "^1.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "gitHead": "3680fde21180fcb6503fd9781f956e82972543c6"
